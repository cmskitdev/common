{
  "name": "@cmskit/common",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "description": "Common utilities for the cmskit project",
  "keywords": [
    "cmskit",
    "common",
    "utilities"
  ],
  "author": {
    "name": "Matthew Davis",
    "email": "matthew@matthewdavis.io"
  },
  "scripts": {
