  "peerDependencies": {
    "svelte": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "object-inspect": "^1.13.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
